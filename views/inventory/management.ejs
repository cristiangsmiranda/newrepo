<% if (title) { %>
  <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>

<%- messages() %>

<% if (errors && errors.length > 0) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>

<a href="/inv/add-classification">Add New Classification</a><br>
<a href="/inv/add-inventory">Add New Vehicle</a>
<ul>
  <% classifications.forEach(classification => { %>
    <li><%= classification.classification_name %></li>
  <% }) %>
</ul>
